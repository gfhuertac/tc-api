VALUES (@projectId@, @companyId@, '@billingAccountName@', @paymentTermId@, @active@, @salestax@, '@poBoxNumber@', '@billingAccountName@', CURRENT, CURRENT + @durationInYears@ UNITS YEAR, CURRENT, '@userId@', CURRENT, '@userId@', @isManualPrizeSetting@)